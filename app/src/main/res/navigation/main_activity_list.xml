<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_activity_list"
    app:startDestination="@id/conversationListFragment">

    <fragment
        android:id="@+id/conversationListFragment"
        android:name="org.variiance.vcall.conversationlist.ConversationListFragment"
        android:label="conversation_list_fragment">
        <action
            android:id="@+id/action_conversationListFragment_to_conversationListArchiveFragment"
            app:destination="@id/conversationListArchiveFragment"
            app:enterAnim="@anim/slide_from_end"
            app:exitAnim="@anim/slide_to_start"
            app:popEnterAnim="@anim/slide_from_start"
            app:popExitAnim="@anim/slide_to_end" />
        <action
            android:id="@+id/action_conversationListFragment_to_storiesLandingFragment"
            app:destination="@id/storiesLandingFragment" />

        <action
            android:id="@+id/action_conversationListFragment_to_appsFragment"
            app:destination="@id/appsFragment" />
        <action
            android:id="@+id/action_conversationListFragment_to_callLogFragment"
            app:destination="@id/callLogFragment" />
        <action
            android:id="@+id/action_conversationListFragment_to_discoverFragment"
            app:destination="@id/discoverFragment" />

        <action
            android:id="@+id/action_conversationListFragment_to_roomsFragment"
            app:destination="@id/roomsFragment" />
    </fragment>








    <fragment
        android:id="@+id/conversationListArchiveFragment"
        android:name="org.variiance.vcall.conversationlist.ConversationListArchiveFragment"
        android:label="conversation_list_archive_fragment" />





    <fragment
        android:id="@+id/storiesLandingFragment"
        android:name="org.variiance.vcall.stories.landing.StoriesLandingFragment"
        android:label="stories_landing_fragment">
        <action
            android:id="@+id/action_storiesLandingFragment_to_callLogFragment"
            app:destination="@id/callLogFragment" />
        <action
            android:id="@+id/action_storiesLandingFragment_to_discoverFragment"
            app:destination="@id/discoverFragment" />
        <action
            android:id="@+id/action_storiesLandingFragment_to_appsFragment"
            app:destination="@id/appsFragment" />

        <action
            android:id="@+id/action_storiesLandingFragment_to_roomsFragment"
            app:destination="@id/roomsFragment" />

    </fragment>





    <fragment
        android:id="@+id/callLogFragment"
        android:name="org.variiance.vcall.calls.log.CallLogFragment"
        android:label="call_log_fragment">
        <action
            android:id="@+id/action_callLogFragment_to_storiesLandingFragment"
            app:destination="@id/storiesLandingFragment" />
        <action
            android:id="@+id/action_callLogFragment_to_discoverFragment"
            app:destination="@id/discoverFragment" />

        <action
            android:id="@+id/action_callLogFragment_to_appsFragment"
            app:destination="@id/appsFragment" />
        <action
            android:id="@+id/action_callLogFragment_to_createCallLinkBottomSheet"
            app:destination="@id/createCallLinkBottomSheet" />

        <action
            android:id="@+id/action_callLogFragment_to_roomsFragment"
            app:destination="@id/roomsFragment" />

    </fragment>





    <dialog
        android:id="@+id/createCallLinkBottomSheet"
        android:name="org.variiance.vcall.calls.links.create.CreateCallLinkBottomSheetDialogFragment"
        android:label="create_call_link_bottom_sheet">
        <action
            android:id="@+id/action_createCallLinkBottomSheet_to_editCallLinkNameDialogFragment"
            app:destination="@id/editCallLinkNameDialogFragment" />
    </dialog>

    <dialog
        android:id="@+id/editCallLinkNameDialogFragment"
        android:name="org.variiance.vcall.calls.links.EditCallLinkNameDialogFragment"
        android:label="edit_call_link_name_fragment">
        <argument
            android:name="name"
            app:argType="string"
            app:nullable="false" />

    </dialog>

    <fragment
        android:id="@+id/discoverFragment"
        android:name="org.variiance.vcall.discover.DiscoverFragment"
        android:label="fragment_discover"
        tools:layout="@layout/fragment_discover" >

        <action
            android:id="@+id/action_discoverFragment_to_storiesLandingFragment"
            app:destination="@id/storiesLandingFragment" />
        <action
            android:id="@+id/action_discoverFragment_to_callLogFragment"
            app:destination="@id/callLogFragment" />

        <action
            android:id="@+id/action_discoverFragment_to_appsFragment"
            app:destination="@id/appsFragment" />


        <action
            android:id="@+id/action_discoverFragment_to_roomsFragment"
            app:destination="@id/roomsFragment" />

    </fragment>



    <fragment
        android:id="@+id/appsFragment"
        android:name="org.variiance.vcall.apps.AppsFragment"
        android:label="fragment_apps"
        tools:layout="@layout/fragment_apps" >

        <action
            android:id="@+id/action_appsFragment_to_storiesLandingFragment"
            app:destination="@id/storiesLandingFragment" />
        <action
            android:id="@+id/action_appsFragment_to_callLogFragment"
            app:destination="@id/callLogFragment" />

        <action
            android:id="@+id/action_appsFragment_to_discoverFragment"
            app:destination="@id/callLogFragment" />

        <action
            android:id="@+id/action_appsFragment_to_roomsFragment"
            app:destination="@id/roomsFragment" />


    </fragment>

    <fragment
        android:id="@+id/roomsFragment"
        android:name="org.variiance.vcall.rooms.RoomsFragment"
        android:label="fragment_rooms"
        tools:layout="@layout/fragment_rooms" >

        <action
            android:id="@+id/action_roomsFragment_to_storiesLandingFragment"
            app:destination="@id/storiesLandingFragment" />
        <action
            android:id="@+id/action_roomsFragment_to_callLogFragment"
            app:destination="@id/callLogFragment" />

        <action
            android:id="@+id/action_roomsFragment_to_discoverFragment"
            app:destination="@id/callLogFragment" />

        <action
            android:id="@+id/action_roomsFragment_to_appsFragment"
            app:destination="@id/appsFragment" />


    </fragment>

</navigation>